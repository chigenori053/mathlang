[
  {
    "id": "ARITH-MUL-001",
    "category": "arithmetic.multiplication",
    "title": "Left distributive law",
    "pattern": {
      "before": "a * (b + c)",
      "after": "a * b + a * c"
    },
    "explanation": "a を括弧内の各項に掛ける分配法則。"
  },
  {
    "id": "ARITH-MUL-002",
    "category": "arithmetic.multiplication",
    "title": "Right distributive law",
    "pattern": {
      "before": "(a + b) * c",
      "after": "a * c + b * c"
    },
    "explanation": "右側にある c を各項に掛ける分配法則。"
  },
  {
    "id": "ARITH-MUL-003",
    "category": "arithmetic.multiplication",
    "title": "Factor common left multiplier",
    "pattern": {
      "before": "a * b + a * c",
      "after": "a * (b + c)"
    },
    "explanation": "共通因子 a を括り出す。"
  },
  {
    "id": "ARITH-MUL-004",
    "category": "arithmetic.multiplication",
    "title": "Factor common right multiplier",
    "pattern": {
      "before": "a * c + b * c",
      "after": "(a + b) * c"
    },
    "explanation": "右側の共通因子 c を括り出す。"
  }
]
